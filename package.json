{
  "name": "splice",
  "version": "0.0.1",
  "description": "GUI workflow for Front-End developers based on Electron",
  "main": "main.js",
  "scripts": {
    "dev": "browser-sync start --server --no-notify --files '**'",
    "start": "electron .",
    "watch": "watchify js/es6.js -t babelify -o js/index.js --debug --verbose",
    "quickexe": "electron-packager .",
    "package": "electron-packager ./ DemoApps --overwrite --app-version=1.0.0 --platform=win32 --arch=all --out=../DemoApps --version=1.2.1 --icon=./public/img/app-icon.icns"
  },
  "repository": "https://github.com/SuperAL/splice.git",
  "keywords": [
    "Electron",
    "workflow"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "dependencies": {
    "autoprefixer": "~6.1.2",
    "dragula": "^3.7.2",
    "gulp": "~3.9.0",
    "gulp-clean-css": "^3.7.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-htmlmin": "^3.0.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^3.3.0",
    "gulp-postcss": "~6.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-rev": "^8.0.0",
    "gulp-uglify": "^3.0.0",
    "gulp-usemin": "^0.3.28",
    "gulp-usemin2": "^0.2.4",
    "gulp-watch": "^4.3.11",
    "gulp.spritesmith": "^6.5.1",
    "imagemin-pngquant": "^5.0.1",
    "merge-stream": "^1.0.1",
    "vinyl-buffer": "^1.0.0"
  },
  "devDependencies": {
    "electron": "^1.6.11",
    "electron-packager": "^8.7.2",
    "electron-reload": "^1.2.1"
  }
}
